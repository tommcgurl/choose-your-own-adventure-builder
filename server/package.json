{
  "name": "server",
  "version": "1.0.0",
  "description": "CYOAB Server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:watch": "nodemon server.js",
    "dev": "npm-run-all migrate-up seed start:watch",
    "test": "mocha -u tdd",
    "create-migration": "node node_modules/db-migrate/bin/db-migrate create -m ./db/migrations --config ./config/database.json",
    "migrate-up": "node node_modules/db-migrate/bin/db-migrate up -m ./db/migrations --config ./config/database.json",
    "migrate-down": "node node_modules/db-migrate/bin/db-migrate down -m ./db/migrations --config ./config/database.json",
    "seed": "node -r dotenv/config db/seed.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "^0.5.1",
    "mocha": "^6.1.4",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "apollo-server-express": "^2.4.8",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "graphql": "^14.2.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-type-json": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.11.0",
    "uuid": "^3.3.2"
  }
}
