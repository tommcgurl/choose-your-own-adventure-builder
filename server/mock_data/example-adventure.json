      "plot": {
        "blocks": [
          {
            "key": "9b8da",
            "text": "You stumble down the hallway, You see a door ahead. You do not see any light coming out from the bottom of the door, only darkness.",
            "type": "unstyled",
            "depth": 0,
            "inlineStyleRanges": [],
            "entityRanges": [],
            "data": {}
          }
        ],
        "entityMap": {}
      },
      "prompt": {
        "text": "Do you go through the door, or keep going and try to find another way out!?",
        "choices": [
          {
            "text": "Open the Door!",
            "nextBranch": "81f2d8c4-a623-4405-93e3-2688d1f0980d",
            "nextBranchName": "openDoor1"
          },
          {
            "text": "Keep running and try to find another way out!",
            "nextBranch": "c79ad738-a74f-49cf-a35a-b54974f68ef0",
            "nextBranchName": "findAnotherWayOut1"
          },
          {
            "text": "Quickly look at map to find out where door leads.",
